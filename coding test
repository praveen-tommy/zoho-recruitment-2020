#include<stdio.h>
main()
{
  int rows,cols,i,j:
  printf("Enter the Number of rows: ");
  scanf("%d",&rows);
  printf("Enter the Number of columns: ");
  scanf("%d",&cols);
  
  for(i=0;i<=rows;i++)
  {
    for(j=cols;j>=cols-i;j--)
    {
      printf("%d",j);
    }
    for(j=1;j<=cols-i;j++)
    {
      printf("%d",(rows - i + 1));
    }
    print("\n");
  }
  return 0;
}
